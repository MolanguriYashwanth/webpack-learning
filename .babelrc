{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": [
                        "last 1 version",
                        "> 1%",
                        "maintained node versions",
                        "not dead"
                    ]
                }
            }
        ],
        "@babel/preset-react",
        "@babel/typescript"
    ],
    "plugins": [
        "@babel/plugin-syntax-dynamic-import"
    ]
}